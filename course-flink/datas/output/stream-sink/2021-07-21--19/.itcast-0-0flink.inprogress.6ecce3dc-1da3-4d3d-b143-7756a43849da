#!/usr/bin/env bash

ZOOKEEPER_HOME=/export/server/zookeeper

EXEC_CMD=$1

# Start Zookeeper
/usr/bin/ssh node1.itcast.cn "cd ${ZOOKEEPER_HOME};source /etc/profile;${ZOOKEEPER_HOME}/bin/zkServer.sh ${EXEC_CMD}"


